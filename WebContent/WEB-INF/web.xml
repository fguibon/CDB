<?xml version="1.0" encoding="ISO-8859-1"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">


	<welcome-file-list>
		<welcome-file>static/views/dashboard.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>DashboardServlet</servlet-name>
		<servlet-class>com.excilys.servlet.DashboardServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DashboardServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>AddComputerServlet</servlet-name>
		<servlet-class>com.excilys.servlet.AddComputerServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddComputerServlet</servlet-name>
		<url-pattern>/add-computer</url-pattern>
	</servlet-mapping>
</web-app>